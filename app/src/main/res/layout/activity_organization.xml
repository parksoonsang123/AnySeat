<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:padding="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/origin_back"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:src="@drawable/ic_baseline_arrow_back_24"
            android:layout_marginLeft="4dp"
            android:clickable="true"
            android:layout_gravity="center"/>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="모바일 공학과 조직도"
            android:textColor="#000000"
            android:textSize="20dp"
            android:textStyle="bold"
            android:layout_gravity="center"/>


        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="8"/>

    </LinearLayout>

    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">

        <Button
            android:id="@+id/Professor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="지도교수\n 최두현"
            android:background="@drawable/jojikbtn"
            android:textSize="10dp"
            android:textColor="#000000"
            android:textStyle="bold"/>

        <Button
            android:id="@+id/Assistant"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:text="조교\n 오은희"
            android:background="@drawable/jojikbtn"
            android:textColor="#000000"
            android:textStyle="bold"
            android:textSize="10dp"/>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginTop="25dp">

            <Button
                android:id="@+id/President_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginRight="10dp"
                android:text="학생회장\n 이승규"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>



            <Button
                android:id="@+id/VicePresident_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="10dp"
                android:text="학생부회장\n 하혜성"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginHorizontal="4dp"
            android:layout_marginTop="30dp">

            <Button
                android:id="@+id/one"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginRight="2.5dp"
                android:text="1학년 대표\n 박태성"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/two"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:background="@drawable/jojikbtn"
                android:text="2학년 대표\n 박재우"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/three"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:text="3학년 대표\n 조영헌"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/four"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:text="4학년 대표\n 이소윤"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/girl"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:text="여학우 대표\n 성자민"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

        </LinearLayout>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:layout_marginHorizontal="4dp"
            android:layout_marginTop="30dp">

            <Button
                android:id="@+id/Manager_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginRight="2.5dp"
                android:text="총무\n 안승재"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/Planning_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:background="@drawable/jojikbtn"
                android:text="기획부장\n 전지윤"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/Officer_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:text="관리부장\n 권민석"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/Ad_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp"
                android:text="홍보부장\n 최인표"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

            <Button
                android:id="@+id/Academic_Student"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="2.5dp"
                android:text="학술부장\n 장호성"
                android:background="@drawable/jojikbtn"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="10dp"/>

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginTop="30dp"
        android:layout_marginHorizontal="10dp"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/teltel"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/call"
                android:layout_gravity="center"/>

            <TextView
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tel : 053 - 950 - 6571\nFax : 053 - 950 - 6572"
                android:layout_gravity="center"
                android:textColor="#000000"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/location"
                android:layout_gravity="center"/>

            <TextView
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="대구광역시 북구 대학로 80\n경북대학교 IT대학 2호관 303호"
                android:layout_gravity="center"
                android:textColor="#000000"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:src="@drawable/officehour"
                android:layout_gravity="center"/>

            <TextView
                android:layout_marginLeft="20dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="평일 : 09:00 ~ 18:00\n점심시간 : 12:00 ~ 13:00"
                android:layout_gravity="center"
                android:textColor="#000000"/>

        </LinearLayout>

    </LinearLayout>



    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_weight="1"/>


</LinearLayout>